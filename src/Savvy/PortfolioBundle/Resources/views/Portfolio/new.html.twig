{% extends 'SavvyStaticBundle::layout.html.twig' %}
{% block title 'Portfolio' %}
{% block theme 'white-bg' %}
{% block body %}
<h1>Portfolio creation</h1>

<form action="{{ path('admin_portfolio_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_portfolio') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}